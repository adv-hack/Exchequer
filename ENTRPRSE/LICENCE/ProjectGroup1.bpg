#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = CDGENRAT.exe ENTVWLIC.exe SETUP.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

CDGENRAT.exe: CDGENRAT.DPR
  $(DCC)

ENTVWLIC.exe: ENTVWLIC.DPR
  $(DCC)

SETUP.exe: ..\CD\SETUP.DPR
  $(DCC)


