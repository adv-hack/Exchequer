PROJECTS = EntRW.exe RepEngine.dll
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

EntRW.exe: EntRW\MH-DPR\EntRW.dpr
  $(DCC)

RepEngine.dll: RepEngine\MH-DPR\RepEngine.dpr
  $(DCC)


